                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingHorizontal="16dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:text="Add District"
                            android:textStyle="bold"
                            android:textSize="16sp"
                            android:textColor="@color/black"
                        />
                        <ImageView
                            android:id="@+id/addDistrictImageView"
                            android:background="?attr/selectableItemBackgroundBorderless"
                            android:clickable="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/ic_add"
                            android:contentDescription="ADD DISTRICT"
                        />
                    </LinearLayout>
                    <androidx.recyclerview.widget.RecyclerView
                        android:layout_marginTop="10dp"
                        android:id="@+id/preferredDistrictRecyclerView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:clipToPadding="false"
                        android:paddingBottom="10dp"
                        android:visibility="visible"
                        android:nestedScrollingEnabled="false"
                    />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#f0f0f0"
                />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="20dp"
                    android:paddingHorizontal="16dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:text="Add Pincode"
                            android:textStyle="bold"
                            android:textSize="16sp"
                            android:textColor="@color/black"
                            />
                        <ImageView
                            android:id="@+id/addPincodeImageView"
                            android:background="?attr/selectableItemBackgroundBorderless"
                            android:clickable="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/ic_add"
                            android:contentDescription="ADD PINCODE"
                            />
                    </LinearLayout>
                    <androidx.recyclerview.widget.RecyclerView
                        android:layout_marginTop="10dp"
                        android:id="@+id/preferredPincodeRecyclerView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:clipToPadding="false"
                        android:paddingBottom="10dp"
                        android:visibility="visible"
                        android:nestedScrollingEnabled="false"
                    />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#f0f0f0"
                />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="20dp"
                    android:paddingHorizontal="16dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:text="Age Limit"
                            android:textStyle="bold"
                            android:textSize="16sp"
                            android:textColor="@color/black"
                        />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/ageGroupMinorLinearLayout"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginRight="10dp"
                            android:background="@drawable/district_card_border"
                            android:orientation="horizontal">
                            <View
                                android:id="@+id/ageGroupMinorIndicator"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_gravity="center_vertical"
                                android:background="@drawable/custom_radio_button"
                            />
                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="18-45"
                                android:layout_marginLeft="10dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                            />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/ageGroupMajorLinearLayout"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginLeft="10dp"
                            android:background="@drawable/district_card_border"
                            android:orientation="horizontal">
                            <View
                                android:id="@+id/ageGroupMajorIndicator"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_gravity="center_vertical"
                                android:background="@drawable/custom_radio_button"
                            />
                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="45+"
                                android:layout_marginLeft="10dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                            />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#f0f0f0"
                />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="20dp"
                    android:paddingHorizontal="16dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:text="Dose"
                            android:textStyle="bold"
                            android:textSize="16sp"
                            android:textColor="@color/black"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/firstDoseLinearLayout"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginRight="10dp"
                            android:background="@drawable/district_card_border"
                            android:orientation="horizontal">
                            <View
                                android:id="@+id/firstDoseIndicator"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_gravity="center_vertical"
                                android:background="@drawable/custom_radio_button"
                                />
                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="1st Dose"
                                android:layout_marginLeft="10dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                            />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/secondDoseLinearLayout"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginLeft="10dp"
                            android:background="@drawable/district_card_border"
                            android:orientation="horizontal">
                            <View
                                android:id="@+id/secondDoseIndicator"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_gravity="center_vertical"
                                android:background="@drawable/custom_radio_button"
                                />
                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="2nd Dose"
                                android:layout_marginLeft="10dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                            />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#f0f0f0"
                />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="20dp"
                    android:paddingHorizontal="16dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:text="Beneficiaries"
                            android:textStyle="bold"
                            android:textSize="16sp"
                            android:textColor="@color/black"
                        />
                    </LinearLayout>
                    <androidx.recyclerview.widget.RecyclerView
                        android:layout_marginTop="10dp"
                        android:id="@+id/preferredBeneficiaryRecyclerView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:clipToPadding="false"
                        android:paddingBottom="10dp"
                        android:visibility="visible"
                        android:nestedScrollingEnabled="false"
                    />
                    <LinearLayout
                        android:visibility="gone"
                        android:id="@+id/noBeneficiariesFoundCard"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/district_card_border"
                        android:layout_marginBottom="20dp"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:textColor="#DB4437"
                                android:text="No Beneficiaries Found"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                            />
                            <View
                                android:id="@+id/noBeneficiariesFoundCustomRadioButton"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_gravity="center_vertical"
                                android:background="@drawable/custom_radio_button"
                            />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#f0f0f0"
                />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="20dp"
                    android:paddingHorizontal="16dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:text="Preferred Vaccines"
                            android:textStyle="bold"
                            android:textSize="16sp"
                            android:textColor="@color/black"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/covaxinLinearLayout"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginRight="10dp"
                                android:background="@drawable/district_card_border"
                                android:orientation="horizontal">
                                <View
                                    android:id="@+id/covaxinIndicator"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/custom_radio_button"
                                    />
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:text="Covaxin"
                                    android:layout_marginLeft="10dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    />
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/covishieldLinearLayout"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/district_card_border"
                                android:orientation="horizontal">
                                <View
                                    android:id="@+id/covishieldIndicator"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/custom_radio_button"
                                    />
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:text="Covishield"
                                    android:layout_marginLeft="10dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    />
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/sputnikLinearLayout"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginRight="10dp"
                                android:background="@drawable/district_card_border"
                                android:orientation="horizontal">
                                <View
                                    android:id="@+id/sputnikIndicator"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/custom_radio_button"
                                />
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:text="Sputnik V"
                                    android:layout_marginLeft="10dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                />
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/allVaccinesLinearLayout"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/district_card_border"
                                android:orientation="horizontal">
                                <View
                                    android:id="@+id/allVaccinesIndicator"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/custom_radio_button"
                                />
                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:text="Any of these"
                                    android:layout_marginLeft="10dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#f0f0f0"
                />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="20dp"
                    android:paddingHorizontal="16dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:text="Cost Type"
                            android:textStyle="bold"
                            android:textSize="16sp"
                            android:textColor="@color/black"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/freeVaccineLinearLayout"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginRight="10dp"
                            android:background="@drawable/district_card_border"
                            android:orientation="horizontal">
                            <View
                                android:id="@+id/freeVaccineIndicator"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_gravity="center_vertical"
                                android:background="@drawable/custom_radio_button"
                                />
                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="Free"
                                android:layout_marginLeft="10dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/paidVaccineLinearLayout"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginLeft="10dp"
                            android:background="@drawable/district_card_border"
                            android:orientation="horizontal">
                            <View
                                android:id="@+id/paidVaccineIndicator"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_gravity="center_vertical"
                                android:background="@drawable/custom_radio_button"
                                />
                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="Paid"
                                android:layout_marginLeft="10dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#f0f0f0"
                />

            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</LinearLayout>